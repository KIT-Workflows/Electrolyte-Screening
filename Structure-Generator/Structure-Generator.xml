<WaNoTemplate>
	
	<WaNoRoot name="Structure-Generator">
	<WaNoFile logical_filename="structures.tar.xz"  name="structures">Choose the .tar file</WaNoFile>
	
		<WaNoMultipleOf name="Add-Molecules">
 			<Element id="0">
		 		<WaNoFloat name="distance">2.0</WaNoFloat>
				<WaNoString name="mol-name">name</WaNoString>
 			</Element>
 		</WaNoMultipleOf>

	</WaNoRoot>
	
	<WaNoExecCommand>
	source /home/ws/gt5111/miniconda3/etc/profile.d/conda.sh
	conda activate
	python attach.py 
	</WaNoExecCommand>
	
	<WaNoInputFiles>
		<WaNoInputFile logical_filename="attach.py">attach.py</WaNoInputFile>
	</WaNoInputFiles>

	<WaNoOutputFiles>
		<WaNoOutputFile>add_mol.xyz</WaNoOutputFile>	
    </WaNoOutputFiles>

</WaNoTemplate>
